# Backend API URL
REACT_APP_BACKEND_URL=http://localhost:3001

# HubSpot Configuration
REACT_APP_HUBSPOT_PORTAL_ID=your_portal_id

# WebSocket URL (usually same as backend)
REACT_APP_WS_URL=ws://localhost:3001

# Environment
REACT_APP_ENV=development

# Twilio Configuration (for WebRTC)
REACT_APP_TWILIO_ACCOUNT_SID=your_account_sid

# Feature Flags
REACT_APP_ENABLE_DEBUG_LOGS=true
REACT_APP_ENABLE_CALL_RECORDING=true
REACT_APP_ENABLE_CONFERENCE=true